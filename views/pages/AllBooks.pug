extends ../components/layout 
block content
    //- console.log('\x1b[36m%s\x1b[0m', "From the AllBooks template: ", books)
    h1 All Books
    p
        a.button(href='books/new') Create New Book
    table
        thead
            tr
                th Title
                th Author
                th Genre
                th Year
        tbody
            each book in books
                tr
                    td
                        a(href='books/' + book.id)= book.title
                    td= book.author
                    td= book.genre
                    td= book.year